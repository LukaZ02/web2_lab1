<div class="main-container-index">
    <div class="counter-container">
        <h1 id="counter-name">Tickets generated:</h1>
        <div class="counter">
            <% totalTickets.forEach(function(number) { %>
                <h1 class="number"><%=number%></h1>
            <% }); %>
        </div>
    </div>
</div>




